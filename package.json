    "dotenv": "16.0.3",
    "express": "4.16.1",
    "helmet": "6.0.1",
    "http-errors": "1.6.3",
    "jsonwebtoken": "9.0.0",
    "mongoose": "6.9.0",
    "morgan": "1.9.1",
    "passport": "0.6.0",
    "passport-jwt": "4.0.1"
  },
  "devDependencies": {
    "@types/assert": "^1.5.6",
    "@types/chai": "4.3.4",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.16",
