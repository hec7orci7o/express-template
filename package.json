{
  "name": "express-template",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": "20.x",
    "npm": "9.8.1"
  },
  "scripts": {
    "start": "node build/app.min.js",
    "build": "tsc --project tsconfig.build.json && tsc-alias && esbuild build/app.js --bundle --minify --platform=node --outfile=build/app.min.js && copyfiles -u 1 src/public/**/* build/",
    "dev": "ts-node-dev -r tsconfig-paths/register -r dotenv/config src/app.ts",
    "test": "nyc mocha --exit",
    "lint": "prettier --write src/**/*.ts && eslint src",
    "prepare": "husky install"
  },
  "dependencies": {
    "body-parser": "1.20.2",
    "copyfiles": "2.4.1",
    "cors": "2.8.5",
    "crypto": "1.0.1",
    "dayjs": "1.11.9",
    "debug": "4.3.4",
    "dotenv": "16.3.1",
    "express": "4.18.2",
    "express-rate-limit": "6.9.0",
    "express-slow-down": "1.6.0",
    "helmet": "7.0.0",
    "http-errors": "2.0.0",
    "jsonwebtoken": "9.0.1",
    "mongoose": "7.4.5",
    "yup": "1.2.0"
  },
  "devDependencies": {
    "@types/chai": "4.3.5",
    "@types/cors": "2.8.13",
    "@types/express": "4.17.17",
    "@types/express-slow-down": "1.3.2",
    "@types/helmet": "4.0.0",
    "@types/http-errors": "2.0.1",
    "@types/jsonwebtoken": "9.0.2",
    "@types/mocha": "10.0.1",
    "@types/node": "20.5.6",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "6.4.1",
    "chai": "4.3.8",
    "chai-http": "4.4.0",
    "esbuild": "0.19.2",
    "eslint": "8.48.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-config-standard-with-typescript": "39.0.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-n": "16.0.2",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-promise": "6.1.1",
    "husky": "8.0.3",
    "lint-staged": "14.0.1",
    "mocha": "10.2.0",
    "nyc": "15.1.0",
    "prettier": "3.0.2",
    "supertest": "6.3.3",
    "ts-node-dev": "2.0.0",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.2.2"
  },
  "lint-staged": {
    "*.ts": "npm run lint"
  }
}
